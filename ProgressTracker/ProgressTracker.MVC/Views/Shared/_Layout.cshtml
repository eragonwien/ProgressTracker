<!DOCTYPE html>
<html>
<head>
   <meta charset="utf-8" />
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1.0" />
   <title>@ViewData["Title"] - ProgressTracker.MVC</title>
   <link rel="stylesheet" href="~/css/site.css" />
   <environment include="Development">
      <link href="~/lib/mui/mui.css" rel="stylesheet" type="text/css" />
      <script src="~/lib/mui/mui.js"></script>
   </environment>
   <environment exclude="Development">
      <link href="//cdn.muicss.com/mui-0.9.42/css/mui.min.css" rel="stylesheet" type="text/css" asp-fallback-href="~/lib/mui/mui.min.css" />
      <script src="//cdn.muicss.com/mui-0.9.42/js/mui.min.js" asp-fallback-src="~/lib/mui/mui.min.js"></script>
   </environment>
</head>
<body>
   <header id="header">
      <partial name="_SideNavPartial" />
   </header>
   <main role="main" class="container" id="main">
      @RenderBody()
   </main>
   <script src="~/js/site.js" asp-append-version="true"></script>
</body>
</html>
