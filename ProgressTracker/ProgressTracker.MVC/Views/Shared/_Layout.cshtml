<!DOCTYPE html>
<html>
<head>
   <meta charset="utf-8" />
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1.0" />
   <title>@ViewData["Title"] - ProgressTracker.MVC</title>
   <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
   <environment include="Development">
      <link href="~/lib/materialize/materialize.css" rel="stylesheet" type="text/css" />
      <script src="~/lib/materialize/materialize.js"></script>
   </environment>
   <environment exclude="Development">
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css" asp-fallback-href="~/lib/materialize/materialize.min.css">
      <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js" asp-fallback-src="~/lib/materialize/materialize.min.js"></script>
   </environment>
   <link rel="stylesheet" href="~/css/site.css" />
   <meta name="theme-color" content="#000000" />
</head>
<body>
   <header id="header">
      <partial name="_SideNavPartial" />
      <partial name="_TopNavbarPartial" />
   </header>
   <main role="main" id="main">
      @RenderBody()
   </main>
   <footer id="footer"></footer>
   <partial name="_ScriptsPartial" />
</body>
</html>
